<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login_control</servlet-name>
  	<display-name>login_control</display-name>
  	<description></description>
  	<servlet-class>property.controller.login_control</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Registration_control</servlet-name>
  	<display-name>Registration_control</display-name>
  	<description></description>
  	<servlet-class>property.controller.Registration_control</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PropertyRegController</servlet-name>
  	<display-name>PropertyRegController</display-name>
  	<description></description>
  	<servlet-class>property.controller.PropertyRegController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login_control</servlet-name>
  	<url-pattern>/login_control</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Registration_control</servlet-name>
  	<url-pattern>/Registration_control</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PropertyRegController</servlet-name>
  	<url-pattern>/PropertyRegController</url-pattern>
  </servlet-mapping>
</web-app>
